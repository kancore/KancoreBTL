<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/kan_nav"
    app:startDestination="@id/devicesbtFragment">
    <fragment
        android:id="@+id/devicesbtFragment"
        android:name="com.kancoreBC.kancorektl.DevicesbtFragment"
        android:label="fragment_devicesbt"
        tools:layout="@layout/fragment_devicesbt" >
        <action
            android:id="@+id/action_devicesbtFragment_to_joystickFragment"
            app:destination="@id/joystickFragment" />
    </fragment>
    <fragment
        android:id="@+id/joystickFragment"
        android:name="com.kancoreBC.kancorektl.JoystickFragment"
        android:label="fragment_joystick"
        tools:layout="@layout/fragment_joystick" >
        <argument
            android:name="device_address"
            app:argType="string" />
    </fragment>
</navigation>